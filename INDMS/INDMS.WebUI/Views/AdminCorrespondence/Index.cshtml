@{
    ViewBag.Title = "Admin Correspondence";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Files
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-user-plus"></i>Admin Files</a></li>
            <li class="active">Admin Correspondence</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Your Page Content Here -->
        <div class="row">
            <div class="col-md-12">
                @if (TempData["MSG"] != null)
                {
                    <div class="alert alert-success alert-dismissable">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        @TempData["MSG"]
                    </div>
                }
                @if (TempData["Error"] != null)
                {
                    <div class="alert alert-danger alert-dismissable">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        @TempData["Error"]
                    </div>
                }

                <div class="box">
                    <div class="box-header">

                    </div><!-- /.box-header -->
                    <div class="box-body">
                        <h1>Under Development</h1>

                        <table id="example1" class="table table-bordered table-striped">
                            <thead>

                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section><!-- /.content -->
</div><!-- /.content-wrapper -->
@section scripts{
    <script>
        $(function () {
            $('li').removeClass("active");
            $('#liAdminFiles').addClass("active")
            $('#liAdminCorrespondence').addClass("active")
            $('#liAdminCorrespondence i').addClass("text-aqua")
        });
    </script>
}